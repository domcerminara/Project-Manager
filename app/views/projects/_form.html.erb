<%= form_for @project do |f| %>
  <%= f.error_messages %>
  
  
  <!-- ******Skill checkboxes******** -->
  	<%= render :partial => 'skill_checkboxes' %>
  <!-- ************************************* -->
  
  
  
  
  <!-- ******Nested attachment form******** -->
  
  
  
  <!-- ************************************* -->
  
  
  <p>
    <%= f.label :name, "Project Name" %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description, :rows => 4, :cols => 35 %>
  </p>
  <p>
    <%= f.label :team_size, "Number of People" %><br />
    <%= f.number_field :team_size %>
  </p>
  <p>
    <%= f.label :duration %><br />
    <%= f.date_select :start_date, :order => [:month, :day, :year] %> - <%= f.date_select :end_date, :order => [:month, :day, :year] %>
  </p>
  <p>
    <%= f.label :hours_per_week, "Work Commitment (hrs/week)" %><br />
    <%= f.number_field :hours_per_week %>
  </p>
  <p>
    <%= f.label :photo, "Project Logo" %><br />
    <%= f.text_field :photo %>
  </p>
  
  <p><%= f.submit %></p>
<% end %>
